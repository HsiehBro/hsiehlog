ZOHO_SMTP_PASS=your-app-password-or-smtp-password
ZOHO_SMTP_HOST=smtp.zoho.com
ZOHO_SMTP_PORT=465
ZOHO_SMTP_SECURE=true
CONTACT_TO=your-address@yourdomain.com
